generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model ttkweaponshu {
  w_id     Int     @id @default(autoincrement())
  w_name   String? @db.VarChar(255)
  w_damage String? @db.VarChar(255)
  w_type   String? @db.VarChar(255)
}

model ttkweaponsen {
  w_id     Int     @id @default(autoincrement())
  w_name   String? @db.VarChar(255)
  w_damage String? @db.VarChar(255)
  w_type   String? @db.VarChar(255)
}
